{
  "name": "recouple",
  "version": "0.2.0",
  "description": "typesafe api routing",
  "repository": "Originate/recouple",
  "main": "lib/index.js",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.11.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-jest": "^21.3.2",
    "flow-copy-source": "^1.2.1",
    "jest": "^21.2.1",
    "koa-route": "^3.2.0",
    "prettier": "^1.8.2"
  },
  "scripts": {
    "build": "yarn build-babel && yarn build-flow",
    "build-babel": "babel src --out-dir lib --ignore '**/*.test.js'",
    "build-flow": "flow-copy-source --ignore '**/*.test.js' src lib",
    "check-all": "flow && yarn format-check && yarn lint && yarn jest",
    "lint": "eslint src/**/*.js",
    "jest": "jest",
    "format": "prettier --write 'src/**/*.js'",
    "format-check": "prettier -l 'src/**/*.js'"
  }
}
